<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>ListCustomerServlet</servlet-name>
		<display-name>ListCustomerServlet</display-name>
		<description></description>
		<servlet-class>comm.example.view.ListCustomerServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>AddCustomerServlet</servlet-name>
		<display-name>AddCustomerervlet</display-name>
		<description></description>
		<servlet-class>comm.example.controller.AddCustomerServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>MyController</servlet-name>
		<display-name>MyController</display-name>
		<description></description>
		<servlet-class>comm.example.controller.MyController</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ListCustomerController</servlet-name>
		<display-name>ListCustomerController</display-name>
		<description></description>
		<servlet-class>comm.example.controller.ListCustomerController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ListCustomerServlet</servlet-name>
		<url-pattern>/list_customer.view</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AddCustomerServlet</servlet-name>
		<url-pattern>/add_customer.do</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>MyController</servlet-name>
		<url-pattern>/MyController</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ListCustomerController</servlet-name>
		<url-pattern>/ListCustomerController</url-pattern>
	</servlet-mapping>
	<welcome-file-list>

		<welcome-file>index.html</welcome-file>
	</welcome-file-list>

</web-app>
